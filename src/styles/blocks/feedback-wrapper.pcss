.feedback-wrapper {
  width: 100%;
  height: 1360px;
  position: relative;
}

.feedback__section {
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  position: absolute;
  top: 0;
  left: 0;
}

.feedback__container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  position: relative;
}